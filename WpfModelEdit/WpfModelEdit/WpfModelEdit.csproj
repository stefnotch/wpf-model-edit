<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <UseWPF>true</UseWPF>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Stefnotch</Authors>
    <Description>Automatically creates WPF windows to edit model classes</Description>
    <Company>n/a</Company>
    <Version>1.0.2</Version>
    <RepositoryUrl>https://github.com/stefnotch/wpf-model-edit</RepositoryUrl>
    <PackageIconUrl>https://raw.githubusercontent.com/stefnotch/wpf-model-edit/master/Icon.png</PackageIconUrl>
    <PackageTags>WPF</PackageTags>
    <PackageLicenseExpression>CC0-1.0</PackageLicenseExpression>
  </PropertyGroup>

</Project>